<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude vPass data for privacy -->
        <exclude domain="database" path="verifd_database" />
        <exclude domain="sharedpref" path="verifd_prefs.xml" />
    </cloud-backup>
    
    <device-transfer>
        <!-- Allow device transfer but exclude sensitive data -->
        <exclude domain="database" path="verifd_database" />
    </device-transfer>
</data-extraction-rules>